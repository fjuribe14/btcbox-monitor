generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tb_operacion {
  id_operacion Int       @id @default(autoincrement())
  in_btcbox    Int       @default(0)
  in_csmf      Int       @default(0)
  in_core      Int       @default(0)
  in_inm       Int       @default(0)
  created_at   DateTime  @default(now())
  updated_at   DateTime?
}

